{
  "name" : "Olwimpics",
  "description" : "Preserving your athletic self-esteem during your daily finger exercises.",
  "website" : "http://fffff.at/olwimpics",
  "version" : "1.0.4",

  "icons" : {
    "16" : "icons/olwimpics16.png",
    "32" : "icons/olwimpics32.png",
    "48" : "icons/olwimpics48.png",
    "64" : "icons/olwimpics64.png",
    "128" : "icons/olwimpics128.png",
    "256" : "icons/olwimpics256.png"
  },

  "manifest_version" : 2,
  "content_security_policy" : "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",

  "web_accessible_resources" : [ 
    "images/blank.png" 
  ],

  "permissions" : [ 
    "tabs", 
    "http://*/*", 
    "https://*/*"
  ],

  "background" : {
    "page" : "background.html"
  },

  "content_scripts" : [ {
    "all_frames" : true,
    "js" : [ "js/jquery-1.7.2.min.js", "js/blocker.js" ],
    "matches" : [ "http://*/*", "https://*/*" ],
    "run_at" : "document_start"
  } ]
}